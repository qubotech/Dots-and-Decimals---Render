# Production Environment Variables
# These optimize React build for production

# Disable source maps (smaller build size)
GENERATE_SOURCEMAP=false

# Disable inline runtime chunk (better caching)
INLINE_RUNTIME_CHUNK=false

# Enable production profiling (optional)
# REACT_APP_PROFILER=false

# Image optimization
IMAGE_INLINE_SIZE_LIMIT=10000

# Disable React DevTools in production
REACT_APP_ENV=production
